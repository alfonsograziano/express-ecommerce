{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "one-var": ["error", "never"],
        "semi": ["error", "never"],
        "no-useless-escape":0
    }
}
